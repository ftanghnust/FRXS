@{
    ViewBag.Title = "AddOrEdit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="searchForm">
<fieldset style="margin:0px 0px 2px 0px;display:none;" id="searchPlan">
    <legend>信息查询</legend>
    UserName：<input type="text" id="txtUserName" />&nbsp;&nbsp;
    UserTrueName：<input type="text" id="txtUserTrueName" />&nbsp;&nbsp;
    Password：<input type="text" id="txtPassword" />&nbsp;&nbsp;
    <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-search" onclick="loadgrid()">查询</a>
    <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-back" onclick="reset()">清空</a>
</fieldset>
</form>
<div id="grid"></div>

@section scripts
{
    <script src="../Content/js/trafficfee.js" type="text/javascript"></script>
}
