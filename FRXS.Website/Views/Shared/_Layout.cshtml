<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link id="themeeasy" href="~/Content/easyui-1.4.3/themes/default/easyui.css" rel="stylesheet" />
    @Scripts.Render("~/Content/easyui")
    @RenderSection("scripts", required: false)
</head>
<body style="margin:0px;padding:2px;overflow-y: hidden;" scroll="no">
    @RenderBody()
</body>
</html>
